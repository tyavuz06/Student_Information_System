@model OBS.MVC.Models.Fakulte
@{
    Layout = "../Shared/_Layout.cshtml";
    ViewBag.Title = "Fakülte Sil";
}
<h2>Fakülte Sil</h2>
<h3>Bu fakülteyi silmek istediğinize emin misiniz?</h3>
<div>
    <h4>Fakulte</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Ad)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Ad)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Adres)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Adres)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Sil" class="btn btn-default" /> |
            @Html.ActionLink("Fakülte Listesine Dön", "FakulteListesi")
        </div>
    }
</div>
